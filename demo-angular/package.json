{
  "scripts": {
    "ns-bundle": "ns-bundle",
    "build.plugin": "cd ../src && npm run build",
    "ci.tslint": "npm i && tslint --config '../tslint.json' 'app/**/*.ts' --exclude '**/node_modules/**'",
    "ci.android.build": "npm run build.plugin && tns prepare android && tns build android",
    "ci.ios.build": "npm run build.plugin && tns prepare ios && tns build ios",
    "publish-ios-bundle": "tns build ios --bundle --publish-app",
    "generate-android-snapshot": "generate-android-snapshot --targetArchs arm,arm64,ia32 --install",
    "start-android-bundle": "tns build android --bundle --run-app",
    "start-ios-bundle": "tns build ios --bundle --run-app",
    "build-android-bundle": "tns build android  --env.snapshot --env.uglify --env.aot",
    "build-ios-bundle": "tns build ios --env.uglify --env.aot",
    "e2e": "tsc -p e2e && mocha --opts ./e2e/config/mocha.opts",
    "e2e-watch": "tsc -p e2e --watch"
  },
  "nativescript": {
    "id": "org.nativescript.demoangular",
    "tns-ios": {
      "version": "5.1.1"
    },
    "tns-android": {
      "version": "5.1.0"
    }
  },
  "dependencies": {
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "nativescript-angular": "^7.2.0",
    "nativescript-theme-core": "1.0.4",
    "nativescript-urlhandler": "*",
    "rxjs": "^6.3.3",
    "tns-core-modules": "^5.1.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~7.2.0",
    "@ngtools/webpack": "~7.2.0",
    "@types/chai": "~4.1.3",
    "@types/mocha": "~5.2.1",
    "@types/node": "^7.0.5",
    "ajv": "^6.8.1",
    "babel-traverse": "6.12.0",
    "babel-types": "6.11.1",
    "babylon": "6.8.4",
    "filewalker": "0.1.2",
    "jasmine-core": "^2.8.0",
    "karma": "^3.0.0",
    "karma-jasmine": "1.1.2",
    "karma-nativescript-launcher": "^0.4.0",
    "lazy": "1.0.11",
    "mocha": "^5.2.0",
    "nativescript-css-loader": "~0.26.1",
    "nativescript-dev-appium": "^5.0.0",
    "nativescript-dev-typescript": "^0.7.3",
    "nativescript-dev-webpack": "^0.19.1",
    "nativescript-unit-test-runner": "0.3.4",
    "tns-platform-declarations": "~5.1.2",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "wd": "~1.10.3"
  }
}
